	keywords = {Algorithm design and analysis, Convex functions, Electric vehicles, IEEE 14 bus system, OPF framework, Optimal power flow, Optimization, Signal processing algorithms, Smart grids, centralized interior point solvers, computational complexity, convex optimization, decentralized online algorithm, distribution network, distribution networks, electric vehicle charging, electric vehicles, fossil fuel, greenhouse gas emission, load flow, nested optimization approach, network constraints, online algorithm, optimal EV charging problem, optimal power flow framework, real residential load profiles, residential area, smart grid, smart power grids, valley-filling, valley-filling algorithms},
	pages = {1073--1083},
}

