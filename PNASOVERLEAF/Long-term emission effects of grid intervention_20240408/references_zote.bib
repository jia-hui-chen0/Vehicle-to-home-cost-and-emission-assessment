@article{lin_emissions_2016,
	title = {Emissions impacts of using energy storage for power system reserves},
	volume = {168},
	issn = {0306-2619},
	url = {https://www.sciencedirect.com/science/article/pii/S0306261916300411},
	doi = {10.1016/j.apenergy.2016.01.061},
	abstract = {Energy storage devices, such as batteries and flywheels, are promising options for providing operating reserves due to their fast response and low emissions during operation. However, because of the complex nature of power systems, adding energy storage-based reserves to the power system may not necessarily benefit the environment. In this paper, we analyze these impacts in a test system and identify important drivers that affect the environmental outcomes. Dispatch results are obtained by solving an optimal power flow (OPF) problem and are used to determine emissions. We find that the impacts of adding energy storage are highly case-dependent. In systems with high renewable penetration levels and significant renewable curtailment, adding energy storage reduces emissions; in other systems, the impacts on emissions could be positive, neutral, or negative. The analyses presented in this paper show that policies to procure energy storage as a means to reduce emissions may actually lead to increased system-wide emissions if current dispatch algorithms are used. We also explore the impacts of modifying the dispatch algorithm to ensure system emissions with energy storage are no worse than system emissions without energy storage.},
	language = {en},
	urldate = {2022-07-31},
	journal = {Applied Energy},
	author = {Lin, Yashen and Johnson, Jeremiah X. and Mathieu, Johanna L.},
	month = apr,
	year = {2016},
	keywords = {Emissions, Energy storage, Operating reserves, Optimal power flow},
	pages = {444--456},
}

@article{zhou_multistage_2021,
	title = {Multistage {Transmission}-{Constrained} {Unit} {Commitment} {With} {Renewable} {Energy} and {Energy} {Storage}: {Implicit} and {Explicit} {Decision} {Methods}},
	volume = {12},
	issn = {1949-3037},
	shorttitle = {Multistage {Transmission}-{Constrained} {Unit} {Commitment} {With} {Renewable} {Energy} and {Energy} {Storage}},
	doi = {10.1109/TSTE.2020.3031054},
	abstract = {Generation scheduling decision-making of power systems with renewable energy and energy storage (ES) is a multistage stochastic programming problem in nature, in which unit commitment (UC) decisions have to be made one day ahead before uncertainties are revealed, and hourly economic dispatch (ED) decisions are successively determined when real uncertainty realizations are observed gradually (i.e., nonanticipativity). To this end, inappropriate ED decisions at current hour may cause infeasibility of future ED decisions (i.e., robustness). Thus, how to properly schedule thermal unit outputs and ES charging/ discharging power against uncertainties becomes an important and urgent issue. In this paper, two mixed-integer linear programming (MILP) methods are proposed to solve the scheduling problem of thermal units and ESs with uncertainties while ensuring solution robustness and nonanticipativity: explicit and implicit decision methods. Specifically, explicit decision method directly assumes affine policies between decision variables and uncertainty realizations; implicit decision method explores safe ranges of thermal unit outputs and ES state-of-charge (SOC) levels to guarantees feasibility of future ED solutions. Both methods can guarantee the nonanticipativity and robustness of multistage solutions. Numerical tests illustrate effectiveness of the proposed methods.},
	number = {2},
	journal = {IEEE Transactions on Sustainable Energy},
	author = {Zhou, Yuzhou and Zhai, Qiaozhu and Wu, Lei},
	month = apr,
	year = {2021},
	note = {Conference Name: IEEE Transactions on Sustainable Energy},
	keywords = {Energy storage, Optimal scheduling, Power systems, Renewable energy sources, Robustness, Transmission-constrained unit commitment, Uncertainty, affine policy, energy storage, implicit decision, renewables},
	pages = {1032--1043},
}

@incollection{chondrogiannis_chapter_2021,
	title = {Chapter 8 - {Power} system flexibility: {A} methodological analytical framework based on unit commitment and economic dispatch modelling},
	isbn = {978-0-12-821838-9},
	shorttitle = {Chapter 8 - {Power} system flexibility},
	url = {https://www.sciencedirect.com/science/article/pii/B9780128218389000086},
	abstract = {The decarbonization policies implemented around the world have led to an ever-increasing penetration of variable Renewable Energy Sources (RES) posing new challenges to modern power systems in terms of adequacy and operational security. Flexibility has emerged as key concept in this new reality, albeit it remains without a universally agreed definition or a concrete assessment framework. This chapter contributes in filling this gap, presenting a step-wise methodological framework for assessing power system flexibility. It combines preliminary model independent analyses with simulations based on a Unit Commitment and Economic Dispatch (UCED) model, representing the sequence of Day-Ahead, Intraday and Balancing Markets, along with System’s operational constraints such as Reserve requirements. Monte-Carlo simulations are employed for capturing the effects of RES and load variability, different hydro conditions, and contingencies in generating assets. A discussion on some key modeling challenges, such as of storage, Demand Response (DR), level of aggregation of generating units, maintenance requirements, and temperature dependency of demand is also provided. The main focus of the work is on the development of an appropriate set of indicators in order to infer meaningful information on power system flexibility needs, challenges, available resources and potential solutions. The developed framework sets a basis for resolving widespread misconceptions such as the non-availability of energy versus the non-availability of adequate level of reserves, or the lack of generating capacity versus the lack of flexibility resources. It also goes beyond traditional adequacy indicators, such as Expected Energy Not Served (EENS) and Loss of Load Expectation (LOLE) in two main streams. First, indicators are developed to gain insights on potential undesired effects in the behavior of power markets, such as the probability of negative prices or high price volatility. Second, the probability of RES curtailment resulting from inadequate flexibility in the power system is assessed. Both can be significant to regulators and market operators, e.g. for developing new market products aiming at enhancing the provision of flexibility, and/or policy makers for assessing the capability of contemporary market architectures to drive the long-term policy goals regarding decarbonization. Finally, the methodological interrelations between flexibility studies and operational security assessments, such as stability analyses, are briefly discussed. Areas of improvement of the presented model are identified, while future challenges in flexibility assessments are qualitatively presented in the context of further decarbonization and decentralization of the power systems, including sector coupling, sectoral integration, modeling of electro-mobility, and peer-to-peer transaction schemes.},
	language = {en},
	urldate = {2022-07-31},
	booktitle = {Mathematical {Modelling} of {Contemporary} {Electricity} {Markets}},
	publisher = {Academic Press},
	author = {Chondrogiannis, Stamatios and Poncela-Blanco, Marta and Marinopoulos, Antonios and Marneris, Ilias and Ntomaris, Andreas and Biskas, Pandelis and Bakirtzis, Anastasios},
	editor = {Dagoumas, Athanasios},
	month = jan,
	year = {2021},
	doi = {10.1016/B978-0-12-821838-9.00008-6},
	keywords = {Demand response, Flexibility, Indicators, Monte-Carlo, UCED},
	pages = {127--156},
}

