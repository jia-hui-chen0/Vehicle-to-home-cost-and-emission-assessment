	pages = {3292--3297},
	file = {Full Text PDF:C\:\\Users\\74770\\Zotero\\storage\\9M2F9KTJ\\Hinkel et al. - 2014 - Coastal flood damage and adaptation costs under 21.pdf:application/pdf;Snapshot:C\:\\Users\\74770\\Zotero\\storage\\QKG49F7Z\\3292.html:text/html},
