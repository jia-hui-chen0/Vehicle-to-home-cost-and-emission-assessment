@article{craig_retrospective_2018,
	title = {A retrospective analysis of the market price response to distributed photovoltaic generation in {California}},
	volume = {121},
	issn = {0301-4215},
	url = {https://www.sciencedirect.com/science/article/pii/S0301421518303781},
	doi = {10.1016/j.enpol.2018.05.061},
	abstract = {Due to rapid growth of distributed solar photovoltaic (PV) capacity in the U.S., numerous “value of solar” studies have attempted to quantify avoided costs associated with distributed PV. One such avoided cost that has received little attention is the market price response, or how distributed PV generation reduces utilities’ procurement costs and, consequently, consumers’ costs through reduced wholesale electricity prices in the short-term. We quantify the reduction in day-ahead wholesale electricity prices to distributed PV generation in California (CA) from 2013 through 2015. Using a database of all distributed PV systems in the three CA investor owned utilities, we estimate historic hourly distributed PV generation using three methods that we validate with metered generation from 205 PV systems. Via multiple linear regression, we then estimate electricity price reductions due to distributed PV generation. Across the three methods used to estimate PV generation, distributed PV generation reduced hourly median (mean) wholesale electricity prices by up to \$2.7–3.1/MWh (\$2.9–3.2/MWh) (\$2015), or by 7–8\% (8–9\%). Lower wholesale prices reduced utilities’ energy procurement costs in the day-ahead market by up to \$650–730 million (\$2015) from 2013 through 2015. These avoided costs are similar to other avoided costs commonly included in value of solar studies.},
	urldate = {2024-05-07},
	journal = {Energy Policy},
	author = {Craig, Michael T. and Jaramillo, Paulina and Hodge, Bri-Mathias and Williams, Nathaniel J. and Severnini, Edson},
	month = oct,
	year = {2018},
	keywords = {Merit order effect, Value of solar, Distributed solar, Market price response},
	pages = {394--403},
	file = {Full Text:C\:\\Users\\74770\\Zotero\\storage\\ENNVKTBW\\Craig et al. - 2018 - A retrospective analysis of the market price respo.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\74770\\Zotero\\storage\\UZJB2STJ\\S0301421518303781.html:text/html},
}

