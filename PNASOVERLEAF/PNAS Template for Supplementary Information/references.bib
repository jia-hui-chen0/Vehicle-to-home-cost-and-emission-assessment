	pages = {104817},
	file = {Vanatta 等 - 2022 - The costs of replacing coal plant jobs with local .pdf:C\:\\Users\\74770\\Zotero\\storage\\33CPNQED\\Vanatta 等 - 2022 - The costs of replacing coal plant jobs with local .pdf:application/pdf},
}

