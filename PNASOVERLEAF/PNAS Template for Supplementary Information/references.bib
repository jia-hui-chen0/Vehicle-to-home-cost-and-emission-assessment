	keywords = {Electric vehicle, Aggregators, Decentralized charging control, Load valley filling, Probability transition matrix, Smart grid},
	pages = {1--7},
	file = {ScienceDirect Snapshot:C\:\\Users\\74770\\Zotero\\storage\\E84R6G77\\S0378779615000802.html:text/html},
}

@article{jian_high_2017,
	title = {High efficient valley-filling strategy for centralized coordinated charging of large-scale electric vehicles},
