	keywords = {Electric vehicle, Developing country, Global warming potential, Greenhouse gas emissions},
	pages = {15--22},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\74770\\Zotero\\storage\\5W4W7W5H\\Onn 等。 - 2018 - Greenhouse gas emissions associated with electric .pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\74770\\Zotero\\storage\\L9NJ9TM7\\S1361920916308823.html:text/html},
