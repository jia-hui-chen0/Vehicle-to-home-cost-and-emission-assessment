@article{strupeit_overcoming_2016,
	series = {Advancing {Sustainable} {Solutions}: {An} {Interdisciplinary} and {Collaborative} {Research} {Agenda}},
	title = {Overcoming barriers to renewable energy diffusion: business models for customer-sited solar photovoltaics in {Japan}, {Germany} and the {United} {States}},
	volume = {123},
