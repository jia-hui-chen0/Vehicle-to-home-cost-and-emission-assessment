@article{weis_emissions_2015,
	title = {Emissions and {Cost} {Implications} of {Controlled} {Electric} {Vehicle} {Charging} in the {U}.{S}. {PJM} {Interconnection}},
	volume = {49},
	issn = {0013-936X, 1520-5851},
	url = {https://pubs.acs.org/doi/10.1021/es505822f},
	doi = {10.1021/es505822f},
	abstract = {We develop a unit commitment and economic dispatch model to estimate the operation costs and the air emissions externality costs attributable to new electric vehicle electricity demand under controlled vs uncontrolled charging schemes. We focus our analysis on the PJM Interconnection and use scenarios that characterize (1) the most recent power plant ﬂeet for which suﬃcient data are available, (2) a hypothetical 2018 power plant ﬂeet that reﬂects upcoming plant retirements, and (3) the 2018 ﬂeet with increased wind capacity. We ﬁnd that controlled electric vehicle charging can reduce associated generation costs by 23\%−34\% in part by shifting loads to lower-cost, higher-emitting coal plants. This shift results in increased externality costs of health and environmental damages from increased air pollution. On balance, we ﬁnd that controlled charging of electric vehicles produces negative net social beneﬁts in the recent PJM grid but could have positive net social beneﬁts in a future grid with suﬃcient coal retirements and wind penetration.},
	language = {en},
	number = {9},
	urldate = {2020-10-06},
	journal = {Environmental Science \& Technology},
	author = {Weis, Allison and Michalek, Jeremy J. and Jaramillo, Paulina and Lueken, Roger},
	month = may,
